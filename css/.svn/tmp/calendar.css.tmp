.month{text-align:center}
.prev,.next {text-align:center}
td.col{vertical-align: top;margin:2px;}
div.title {background: #ECFDED;}
div.day {font-size: 22px;text-align: center;height: 70px;line-height: 70px;vertical-align: center;}
.outMonth {color: #CCC;background: white;}
.inMonth {color: black;background: #CCC;}
.today .day{/*color:red*/ font-weight: bold;font-size: 36px}
div.title .event{float:right;margin-right:5px}
.count0{color:#222222;}
.count1{color:red;}
.count2{color:red;}
.count3{color:red;}


.timeSlot{text-align:left;cursor: pointer;}
.minuts{text-align:left;}
.slotList tr{border-top:1px solid lightgray}
.myEvent thead tr, .slotList thead tr{background:url('../images/base/bg_title_gray.jpg') bottom repeat-x #eee; border-radius: 13px; padding: 10px 0;font-size:16px;color: #494949;text-shadow: 0px 1px 0px #fff;}
.timeSlot{background:lightgray}
.setted{background:green}
.createTs,.myList{cursor: pointer;}
.tl{text-align:left}
.Booked{background:red}
.myEvent tr.Opened{cursor: pointer; padding: 5px; border-bottom: 1px solid #ccc;}
.timeSlot.Ended{cursor:auto;background:gray;}
.myEvent .Ended{cursor:auto;background:gray;}

