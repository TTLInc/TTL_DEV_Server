.how_it_works_left01 h2 a, .how_it_works_left02 h2 a, .how_it_works_left03 h2 a{ margin-bottom:20px;}
.ly-index .header .top_navi .signinbtn{ width:179px !important}

@media only screen and (max-width: 768px) {
.container, .partner_section_main, .testimonials_main{ /*max-width:1348px;*/max-width:100%;overflow:hidden; }
.wrapper{width:auto; overflow:hidden;}
}
