@charset "utf-8";
/* CSS Document */
.nobg{background:none!important;padding:0!important;white-space:nowrap;}
.raduisSelect{ color:#919191; margin-left:0; font-size:12px; font-style:italic; padding:5px 10px; width:98px;}
.w78{ width:78px;}
.input_text{ border-radius:5px; padding:5px; background:#f4f4f4; width:186px; border:0; margin:0; font-size:14px; color:#666666;}
.textarea{ border-radius:5px; padding:2px 5px; line-height:18px; height:72px; background:#f4f4f4; width:186px; border:0; margin:0; font-size:14px; color:#666666;}
.btn_red{ width:70px; height:30px; border-radius:8px; border:1px solid #999999; background-image: -moz-linear-gradient(top, #c74b4b, #800303); background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, #c74b4b), color-stop(1, #800303)); filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#c74b4b', endColorstr='#800303', GradientType='0'); box-shadow:0 3px 2px rgba(0,0,0,0.6); font-size:14px; font-weight:600; color:#fff; text-align:center; line-height:30px; cursor:pointer;}
.btn_yellow{ display:inline-block; width:90px; height:30px; border-radius:20px; border:1px solid #999999; background-image: -moz-linear-gradient(top, #ffd92c, #c1990e); background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, #ffd92c), color-stop(1, #c1990e)); filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ffd92c', endColorstr='#c1990e', GradientType='0'); box-shadow:0 3px 2px rgba(0,0,0,0.6); text-shadow:1px 1px 1px #ffffff; font-size:14px; font-weight:600; color:#3a3a3a; text-align:center; line-height:30px; cursor:pointer;}
.btn_red_big,.btn_blue_big{ display:inline-block; padding:0 15px; height:25px; margin-left:12px; border-radius:20px; border:1px solid #999999; background-image: -moz-linear-gradient(top, #c74b4b, #800303); background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, #c74b4b), color-stop(1, #800303)); filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#c74b4b', endColorstr='#800303', GradientType='0'); box-shadow:0 3px 2px rgba(0,0,0,0.6); font-size:12px; font-weight:600; color:#fff; text-align:center; line-height:25px; cursor:pointer;}
.btn_blue_big{ background-image: -moz-linear-gradient(top, #26729c, #203240); background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, #26729c), color-stop(1, #203240)); filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#26729c', endColorstr='#203240', GradientType='0');}

.search{ padding:20px 35px 30px; overflow:hidden; zoom:1;}
.search_top{ height:40px; line-height:40px; color:#133e5f; font-size:36px; font-weight:bold; border-bottom:1px solid #e6e6e6;}
.search_mid{ padding-top:20px; padding-bottom:10px; overflow:hidden; zoom:1;}
.search_lf{ float:left; display:inline; width:244px; border-radius:15px; background-image: -moz-linear-gradient(top, #48c7f6, #22aef1); background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, #48c7f6), color-stop(1, #22aef1)); filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#48c7f6', endColorstr='#22aef1', GradientType='0'); box-shadow:0 3px 2px rgba(0,0,0,0.6); padding:15px 0;}
.search_lf_top{ border-bottom:1px solid #73d0f7;}
.search_lf_top span{ display:block; height:44px; padding:0 25px; line-height:44px; border-bottom:1px solid #2f9fcd; font-size:18px; color:#ffffff; text-shadow:1px 1px 1px #cccccc; font-weight:bold;}
.search_lf_mid{ width:196px; margin:0 auto;}
.search_lf_mid dl,.search_lf_mid dt,.search_lf_mid dd{ width:196px;}
.search_lf_mid dl{ margin-top:15px;}
.search_lf_mid dl.mt0{ margin-top:0;}
.search_lf_mid dt span{ display:inline-block; background:url(../images/search/icon_search.png) right top no-repeat; height:23px; line-height:23px; text-transform:uppercase; color:#0B3D5F; font-size:14px; font-weight:bold; padding-right:24px;}
.search_lf_mid dt span.c133e5f{ color:#133e5f;}

.search_lf_mid .icon_1{ background:none;/*background-position:right 0;*/}
.search_lf_mid .icon_2{ background-position:right -23px;}
.search_lf_mid .icon_3{ background-position:right -46px;}
.search_lf_mid .icon_4{ background-position:right -92px;}
.search_lf_mid .icon_5{ background-position:right -69px;}
.search_lf_mid .icon_6{ background-position:-5px -115px; padding:0 0 0 18px; text-transform:none;}
.search_lf_mid .icon_7{ background-position:-5px -138px; padding:0 0 0 18px; text-transform:none;}
.search_lf_mid .icon_8{ background-position:right -161px;}
.search_lf_mid .icon_9{ background-position:right -184px;}
.search_lf_mid .icon_none{ background:none;}
.search_lf_mid dd{ margin-top:4px;}
.search_lf_mid dd.white{ color:#ffffff; vertical-align:middle; font-size:12px;}
.search_lf_mid dd .check{ display:inline-block; height:23px; line-height:23px; color:#c3f0f9; font-size:14px; font-weight:bold; text-transform:uppercase; background-image:url(../images/search/icon_search.png); background-repeat:no-repeat; padding-right:24px;}
.search_lf_mid dd .langu{ border-radius:5px; padding:0 0 5px 5px; background:#ffffff;}
.search_lf_mid dd .langu span{ display:inline-block; margin:5px 6px 0 0; border-radius:3px; height:18px; background-color:#b2b2b2; font-size:11px; font-family:Arial;}
.search_lf_mid dd .langu span.text{ margin:0; line-height:18px; color:#ffffff; padding:0 5px; border-radius:3px 0 0 3px;}
.search_lf_mid dd .langu a{ display:inline-block; border-radius:0 3px 3px 0; background-color:#7a7a7a; height:18px; padding:0 3px; line-height:18px; color:#ffffff; text-decoration:none;}
.search_lf_mid dd .langu a:hover{ text-decoration:none;}
.search_lf_mid dt .check{ display:inline-block; height:23px; line-height:23px; color:#c3f0f9; font-size:14px; font-weight:bold; text-transform:uppercase; background-image:url(../images/search/icon_search.png); background-repeat:no-repeat; padding-right:24px;}

.search_lf_mid .line2{ margin:20px 0 5px; border-bottom:1px solid #73d0f7; border-top:1px solid #2f9fcd;}
.search_lf_mid .span{display:block; height:44px; line-height:44px; font-size:18px; color:#ffffff; text-shadow:1px 1px 1px #cccccc; font-weight:bold;}
.search_lf_mid .submit_btn{ padding:20px 0 10px; text-align:right;}
.search_rt{ float:right; display:inline; width:748px;}
.search_rt_top{ border-radius:8px;}
.search_rt_mid{ padding-top:35px;}
.search_rt_mid_t{ color:#5a98a6; font-size:14px; overflow:hidden; zoom:1;}
.search_rt_mid_t a{ color:#5a98a6;}
.search_rt_mid_t .number{ color:#365d66;}
.search_rt_mid_t_lf{ float:left; display:inline;}
.search_rt_mid_t_rt{ float:right; display:inline;}
.lh30{ height:30px; line-height:30px;}
.mr30{ margin-right:30px;}
.mr10{ margin-right:10px;}

.v_ajax_page{ height:30px; color:#999999; font-size:14px;}
.v_ajax_page span{ display:inline-block; height:27px; line-height:27px; border-radius:3px; /*margin-right:12px;*/ margin:0 3px; padding:0 8px;}
.v_ajax_page a{ display:inline-block; height27px; border-radius:3px; /*margin-right:12px;*/ margin:0; padding:0 6px; line-height:27px; color:#1374a0;}
.v_ajax_page a:hover{ text-decoration:underline;}
.v_ajax_page .current{ background:#047cb5; color:#ffffff; box-shadow:0 3px 2px rgba(0,0,0,0.6); background-color:#0584c1; font-weight:bold;}

.result_title{ margin-top:10px; height:30px; line-height:30px; font-size:18px; font-weight:bold; color:#0d2a41;}
.result_list{ overflow:hidden; zoom:1;}
.result_list dl{ border-radius:5px; margin-top:1px; padding:15px 15px 11px; background:#ececec; overflow:hidden; zoom:1;}
.result_list dl.none{ background:none;}
.result_list dt{ float:left; display:inline; width:164px; /*height:90px;*/ position:relative;}
.result_list dt .icon_play{ position:absolute; left:57px; top:21px; background:url(../images/search/icon_play.png) no-repeat; width:49px; height:48px;}
.result_list dt img{ border-radius:2px; width:163px; /*height:90px;*/ vertical-align:top; border:0;max-height: 180px}
.result_list dd{ float:right; display:inline; width:540px;}
.result_list dd .dd_top,.result_list dd .dd_bot{overflow:hidden; zoom:1}
.result_list dd .dd_bot{ margin-top:4px;}
.result_list dd .dd_lf{ float:left; display:inline; width:400px;}
.result_list dd .dd_lf h1{height:28px; line-height:28px; font-size:14px; color:#424242; font-weight:bold; font-family:Arial;}
.result_list dd .dd_lf font{ color:#037898;}
.result_list dd .dd_lf p{ height:32px; line-height:16px; font-size:12px; color:#666666; overflow:hidden;}
.result_list dd .dd_lf p span{ color:#333333; font-weight:bold;}
.result_list dd .dd_rt{ float:right; display:inline; width:140px; height:55px; text-align:center; line-height:55px; color:#037898; font-size:20px; font-weight:bold;}
.result_list dd .btn_group{ float:right; display:inline;}
.stars{ float:left; display:inline; background:url(../images/search/stars.png) 0 -30px repeat-x;float:left;height:30px;width:150px;position:relative;overflow:hidden;}
.stars .current_rating{background:url(../images/search/stars.png) repeat;display:block;height:30px;left:0;position:absolute;text-indent:-9000px;z-index:1}
.stars a{display:block;height:30px;position:absolute;text-decoration:none;text-indent:-9000px;width:30px;z-index:20}
.stars a:hover{background:url(../images/search/stars.png) repeat;left:0;z-index:1}
.stars a.one-star{left:0px}
.stars a.two-stars{left:30px}
.stars a.three-stars{left:60px}
.stars a.four-stars{left:90px}
.stars a.five-stars{left:120px}
.stars a.one-star:hover{width:30px}
.stars a.two-stars:hover{width:60px}
.stars a.three-stars:hover{width:90px}
.stars a.four-stars:hover{width:120px}
.stars a.five-stars:hover{width:150px} 

.pt20{ padding-top:20px;}

.result_list dd .dd_bot .span{ float:right; display:inline; font-size:12px; font-style:italic; height:30px; line-height:30px; padding:0 15px; color:#777777;}