@charset "utf-8";
/* CSS Document */
.contact_us{ padding:35px 35px 60px;}
.contact_us_top{ height:40px; line-height:40px; color:#133e5f; font-size:24px; font-weight:bold; border-bottom:1px solid #e6e6e6;}
.contact_us_mid{ }
.contact_us_mid p{ padding:30px 30px 0; /*text-indent:2em;*/ color:#585858; font-family:Arial; line-height:18px; font-size:14px;}
