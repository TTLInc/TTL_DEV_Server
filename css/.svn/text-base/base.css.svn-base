/* 浮层 */

#calendarEventDialog{position:absolute;z-index:9999;width:350px;background:url('../images/base/bg_book.jpg') bottom repeat-x #fff;border:1px solid #ccc;padding:10px 20px 12px;box-shadow: 0px 5px 8px #ccc;}

.createTs {font-size: 18px; font-weight: bold; color: #fff; text-shadow: 0px 1px 1px #666;background:url('../images/base/bg_title_book.jpg') bottom repeat-x #00c2ea; border-radius: 13px; padding: 10px 20px; width: 310px; display:block; margin: 0 auto;}

.dayStr {font-size: 16px; color: #077289; text-transform:uppercase; font-weight: bold; text-align:center;}

.statusBar {font-weight: bold; color: #666;}

#eventList{height:300px;overflow:auto;padding:0 5px;}

.event_tle{font-size:15px;}

#eventList li{border-radius: 20px; background-color: #a0a0a0;margin-bottom:10px;padding:10px 20px;}

#eventList li:nth-child(2n){background-color:#ececec;}

.event_arr{position:absolute;width:19px;height:73px;overflow:hidden;background:url('../images/base/event_arrow.png') no-repeat 0 0;bottom:-65px;}

.el_arr{border-radius:30px 30px 30px 12px;-webkit-border-radius:30px 30px 30px 12px;}

.er_arr{border-radius:30px 30px 12px 30px;-webkit-border-radius:30px 30px 12px 30px;}

.el_arr .event_arr{left:-1px;background-position: -19px 0;}

.er_arr .event_arr{right:-1px;}

