#demo4{width:310px !important; height:377px !important; position:absolute !important; /*margin:-31px 0 0 39px ; margin:-44px 0 0 49px ;*/ margin:-43px 0 0 37px;}
#demo5{width:310px !important; height:377px !important; position:absolute !important; /*margin:-28px 0 0 31px ; margin:-41px 0 0 41px ;*/ margin:-37px 0 0 36px; z-index:-1;}

.banner_main{ display:block;}
.banner-main-mobile{ display:none;}

.bx-wrapper .bx-viewport{ background:none; min-height:64px;}

.banner_search_right{ text-align:center;}
.banner_search_right img{ max-width:100%;}
.banner_search_right .register_btn{ width:100%; color:#fff; text-decoration: underline; font-size:15px; margin:15px 0; float:left;}

.howit-video{ text-align:center;}
.howit-video .video_main{ display: inline-block; margin:0 15px;}



